!URxvt*internalBorder: 			20
URxvt.intensityStyles:			false
!URxvt.background: 				#121212
URxvt*background:               rgba:0000/0000/0000/aaaa
URxvt.foreground: 				#BBBBBB
URxvt.depth:					32
URxvt.font: 					xft:Hack Nerd Font Mono:size=9
URxvt.scrollBar:				false
URxvt.cursorColor:				white
URxvt.colorUL:					#61c7c6
URxvt.keysym.Shift-Control-V: 	eval:paste_clipboard
URxvt.keysym.Shift-Control-C: 	eval:selection_to_clipboard
URxvt.iso14755: true
URxvt.iso14755_52: false
URxvt.letterSpace: -1
URxvt.lineSpace: 0

/* See section in sts config.h that describes XResources (~L175) */
st.borderpx: 0
st.font: Hack Nerd Font Mono:size=9:antialias=true:autohint=true

Xft.autohint: 	0
Xft.antialias: 	1
Xft.hinting: 	true
!Xft.hintstyle: 	hintslight
Xft.hintstyle:  hintfull
Xft.dpi: 	96
!Xft.rgba: 	rgb
Xft.rgba: 	none
Xft.lcdfilter: lcddefault

Xcursor.theme: Openzone-Black
