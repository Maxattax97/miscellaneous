# vim:ft=sshconfig
#
# Once you setup a host in here, you can use the following command to
# automatically connect and install your public host key to enable passwordless
# SSH-key logins:
#	 ssh-copy-id <host>

Host *
	AddKeysToAgent yes
	IdentityFile ~/.ssh/id_rsa

#Host maxocull.com
	#HostName maxocull.com
	#User entourage
	#IdentityFile ~/.ssh/id_rsa

##### BATS #####

Host gitea.batswireless.lcl
	HostName gitea.batswireless.lcl
	IdentityFile ~/.ssh/id_ed25519_bats

##### AURA #####

Host aura-lab-dell-xr11
	HostName 10.101.200.50
	User aura-lab-admin
	ForwardAgent yes

Host aura-lab-ru
	HostName 10.101.201.10
	User aura
	ForwardAgent yes

Host aura-lab-ars
	HostName 10.101.201.26
	User aura
